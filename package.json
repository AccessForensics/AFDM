{
  "name": "sku-a-forensic-engine",
  "version": "5.7.6",
  "description": "Hardened mechanical observer for ADA Title III litigation capture.",
  "main": "engine/ect.js",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "playwright": "1.55.1"
  },
  "scripts": {
    "smoke": "node engine/run_smoke.js manifests/smoke.json",
    "smoke:desktop": "node engine/run_smoke_desktop.js manifests/smoke_desktop.json",
    "smoke:mobile": "node engine/run_smoke_mobile.js manifests/smoke_mobile.json",
    "smoke:dual": "npm run smoke:desktop && npm run smoke:mobile",
    "matter": "node triage_runner.js triage_manifest.json",
    "matter:triage": "node triage_runner.js triage_manifest.json",
    "verify:env": "node tools/verify_env.js",
    "gate:smoke": "npm run smoke:dual && npm run verify:env",
    "scan:sku-a:web": "npm run smoke:desktop",
    "scan:sku-b:dual": "npm run smoke:dual",
    "scan:sku-b:web": "npm run smoke:desktop",
    "scan:sku-b:mobile": "npm run smoke:mobile",
    "seal:packet": "node tools/seal_packet.js"
  }
}
